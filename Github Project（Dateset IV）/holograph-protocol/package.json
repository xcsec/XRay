{
  "name": "@holographxyz/holograph-protocol",
  "version": "0.0.1",
  "description": "Holograph Protocol",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "@holographxyz",
  "repository": {
    "type": "git",
    "url": "https://github.com/holographxyz/holograph-protocol.git"
  },
  "homepage": "https://holograph.xyz",
  "keywords": [
    "holograph",
    "holograph protocol"
  ],
  "bugs": {
    "url": "https://github.com/holographxyz/holograph-protocol/issues"
  },
  "main": "index.js",
  "files": [
    "abi",
    "deployments",
    "typechain-types"
  ],
  "scripts": {
    "postinstall": "(ls .env >> /dev/null 2>&1 && echo '.env file already exists') || cp sample.env .env",
    "dev:iterate": "hardhat clean & hardhat typechain",
    "init": "(ls .env >> /dev/null 2>&1 && cp .env ignore.original.env) ; cp sample.env .env",
    "abi": "hardhat abi",
    "build": "hardhat compile",
    "clean-compile": "forge clean ; forge build",
    "clean": "hardhat clean",
    "compile": "forge build",
    "deploy": "hardhat deploy",
    "deploy-x2": "pnpm deploy:localhost && pnpm deploy:localhost2",
    "deploy:localhost": "hardhat deploy --network localhost --no-compile",
    "deploy:localhost2": "hardhat deploy --network localhost2 --no-compile",
    "ganache": ". ./.env ; rm -fr ./ganache/db ; mkdir -p ./ganache/db ; rm ./ganache/keys.json ; ganache-cli --accounts=11 --defaultBalanceEther=10000 --deterministic --mnemonic=\"$MNEMONIC\" --port=8545 --hostname=0.0.0.0 --gasPrice=0 --gasLimit=10000000 --networkId=1338 --chainId=1338 --db=./ganache/db --acctKeys=./ganache/keys.json",
    "ganache2": ". ./.env ; rm -fr ./ganache/db2 ; mkdir -p ./ganache/db2 ; rm ./ganache/keys2.json ; ganache-cli --accounts=11 --defaultBalanceEther=10000 --deterministic --mnemonic=\"$MNEMONIC\" --port=9545 --hostname=0.0.0.0 --gasPrice=0 --gasLimit=10000000 --networkId=1339 --chainId=1339 --db=./ganache/db2 --acctKeys=./ganache/keys2.json",
    "ganache-x2": ". ./.env ; rm -fr ./ganache/db ; rm -fr ./ganache/db2 ; mkdir -p ./ganache/db ; mkdir -p ./ganache/db2 ; rm ./ganache/keys.json ; rm ./ganache/keys2.json ; ganache-cli --accounts=11 --defaultBalanceEther=10000 --deterministic --mnemonic=\"$MNEMONIC\" --port=8545 --hostname=0.0.0.0 --gasPrice=0 --gasLimit=10000000 --networkId=1338 --chainId=1338 --db=./ganache/db --acctKeys=./ganache/keys.json & ganache-cli --accounts=11 --defaultBalanceEther=10000 --deterministic --mnemonic=\"$MNEMONIC\" --port=9545 --hostname=0.0.0.0 --gasPrice=0 --gasLimit=10000000 --networkId=1339 --chainId=1339 --db=./ganache/db2 --acctKeys=./ganache/keys2.json",
    "forge-build": "pnpm run build && forge build",
    "anvil": ". ./.env ; anvil --accounts=11 --block-base-fee-per-gas=0 --mnemonic=\"$MNEMONIC\" --port=8545 --host=0.0.0.0 --chain-id=1338 & anvil --accounts=11 --mnemonic=\"$MNEMONIC\" --block-base-fee-per-gas=0 --port=9545 --host=0.0.0.0 --chain-id=1339",
    "test": "hardhat test",
    "tests": "hardhat test",
    "typechain": "hardhat typechain",
    "verify": "hardhat verify",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lint:fix": "eslint . --ext .ts --config .eslintrc --fix",
    "prettier:check": "prettier --ignore-unknown --check \"deploy/**/*.ts\" \"test/**/*.ts\" \"scripts/**/*.ts\"",
    "prettier:fix": "prettier --loglevel error --ignore-path .gitignore --ignore-path **/lib --write .",
    "prettier:deployments": "hardhat deploymentsPrettier",
    "natspec": "npx natspec-smells &> natspec-smells.report.txt",
    "snapshot": "FOUNDRY_PROFILE=ci forge snapshot --no-match-test \"(FFI|Fork|Fuzz)\" --offline",
    "snapshot:check": "FOUNDRY_PROFILE=ci forge snapshot --no-match-test \"(FFI|Fork|Fuzz)\" --check --offline"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/bignumber": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@ethersproject/units": "^5.7.0",
    "@holographxyz/environment": "0.1.10",
    "@holographxyz/hardhat-deploy-holographed": "^0.0.6",
    "@holographxyz/holograph-genesis": "0.2.2",
    "@holographxyz/networks": "0.2.11",
    "@types/yargs": "^17.0.32",
    "chai-ethers": "^0.0.1",
    "ganache": "^7.9.2",
    "hardhat-preprocessor": "^0.1.5",
    "path": "^0.12.7",
    "truffle-hdwallet-provider": "^1.0.17",
    "web3": "^1.10.4",
    "zksync-web3": "^0.14.4"
  },
  "devDependencies": {
    "@anders-t/ethers-ledger": "^1.0.4",
    "@defi-wonderland/natspec-smells": "^1.1.1",
    "@nomicfoundation/hardhat-foundry": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@nomiclabs/hardhat-etherscan": "3.1.7",
    "@nomiclabs/hardhat-waffle": "2.0.5",
    "@openzeppelin/contracts": "4.8.3",
    "@openzeppelin/contracts-upgradeable": "4.8.3",
    "@tenderly/hardhat-tenderly": "2.1.0",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "6.1.5",
    "@types/chai": "4.3.4",
    "@types/data-urls": "^3.0.4",
    "@types/mocha": "10.0.1",
    "@types/node": "20.12.2",
    "@types/readline-sync": "^1.4.8",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "4.3.7",
    "data-urls": "^4.0.0",
    "dotenv": "^16.4.5",
    "erc721a-upgradeable": "4.2.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-mocha": "^10.4.3",
    "eslint-plugin-prettier": "4.2.1",
    "ethereum-waffle": "3.4.0",
    "ethers": "^5.7.2",
    "ganache-cli": "^6.12.2",
    "hardhat": "2.14.0",
    "hardhat-gas-reporter": "1.0.9",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.3.1",
    "readline-sync": "^1.4.10",
    "solhint": "^3.6.2",
    "solhint-plugin-prettier": "^0.0.5",
    "ts-node": "^10.9.2",
    "typechain": "8.1.1",
